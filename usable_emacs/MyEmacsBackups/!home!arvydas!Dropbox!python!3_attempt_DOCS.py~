# installed venv, chrome driver version 96.0.4664.45
# pip install selenium in virtual env
from selenium import webdriver
from selenium.webdriver.common.keys import Keys
import time

# read the driver and enter website
driver = webdriver.Chrome("/home/arvydas/Dropbox/python/chromedriver")
driver.get("http://www.python.org")
assert "Python" in driver.title # nesuprantu ar reikalingas, nes vesk bet ka - scriptas vaziuos

# find search field
elem = driver.find_element_by_name("q")

# clean the search field
elem.clear()

# enter text into the search field
elem.send_keys("Arvydas")
time.sleep(5)

# press Enter
elem.send_keys(Keys.RETURN)
time.sleep(5)

# close 
driver.close()
